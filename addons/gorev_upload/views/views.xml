<odoo>
  <data>
    explicit list view definition 

    <record model="ir.ui.view" id="gorev_upload.list">
      <field name="name">Görev upload</field>
      <field name="model">gorev_upload.gorev_upload</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="tarih"/>
          <field name="dosyaYukleme"/>
          <field name="dosyaSahibi"/>
        </tree>
      </field>
    </record>


    

    <record model="ir.actions.act_window" id="gorev_upload.action_window">
      <field name="name">Görev upload</field>
      <field name="res_model">gorev_upload.gorev_upload</field>
      <field name="view_mode">tree,form</field>
    </record>

    <record model="ir.actions.server" id="gorev_upload.action_server">
      <field name="name">Görev upload</field>
      <field name="model_id" ref="model_gorev_upload_gorev_upload"/>
      <field name="state">code</field>
      <field name="code">
        action = {
          "type": "ir.actions.act_window",
          "view_mode": "tree,form",
          "res_model": model._name,
        }
      </field>
    </record>

    <menuitem name="Görev upload" id="gorev_upload.menu_root"/>

    <menuitem name="List" id="gorev_upload.menu_1" parent="gorev_upload.menu_root" action="gorev_upload.action_window"/>
    <menuitem name="Server to list" id="gorev_upload.menu_2" parent="gorev_upload.menu_root" action="gorev_upload.action_server"/>

  </data>
</odoo>